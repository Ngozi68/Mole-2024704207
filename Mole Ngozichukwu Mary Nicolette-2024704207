score = int(input("Enter score: "))

# Determine the grade and print YES when it matches
if 70 <= score <= 100:
    print("Grade: A")
    print("YES")
elif 60 <= score <= 69:
    print("Grade: B")
    print("YES")
elif 50 <= score <= 59:
    print("Grade: C")
    print("YES")
elif 40 <= score <= 49:
    print("Grade: D")
    print("YES")
elif 30 <= score <= 39:
    print("Grade: E")
    print("YES")
else:
    print("Invalid score")
    print("NO")
